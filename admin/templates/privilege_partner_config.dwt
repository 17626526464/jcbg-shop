<!doctype html><html><head>{include file ='library/admin_html_head.lbi'}</head><body class="iframe_body"><div class="warpper">    <div class="title">{$lang.partner} - {$ur_here}</div>    <div class="content">        <div class="explanation" id="explanation">            <div class="ex_tit"><i class="sc_icon"></i><h4>{$lang.operating_hints}</h4><span id="explanationZoom" title="{$lang.fold_tips}"></span></div>            <ul>                <li>{$lang.partner_config_content.list.0}</li>                <li>{$lang.partner_config_content.list.1}</li>            </ul>        </div>        <div class="flexilist">            <div class="common-content">                <div class="mian-info">                    <form method="POST" action="" name="theForm111">                        <div class="switch_info">                            <div class="item">                                <div class="label">{$lang.partner_company}：</div>                                <div class="label_value">                                    <input type="text" id="company_name" name="company_name" class="text" value="{$partner_manager_info.PartnerCompany.company_name}" readonly autocomplete="off">                                    <div class="form_prompt"></div>                                </div>                            </div>                            <div class="item">                                <div class="label">{$lang.select_partner_manager}：</div>                                <div class="label_value">                                    <input type="text" id="user_name" name="user_name" class="text" value="{$partner_manager_info.AdminUser.user_name}" readonly autocomplete="off">                                    <div class="form_prompt"></div>                                </div>                            </div>                            <input type="hidden" name="partner_company_config_id" value="{$partner_company_config.id}">                            <div class="item">                                <div class="label">{$lang.partner_manager_audited}：</div>                                <div class="label_value">                                    <div class="checkbox_items">                                        <div class="checkbox_item">                                            <input type="radio" class="ui-radio" name="manager_audited" id="manager_audited_1" value="1" {if $partner_company_config.manager_audited === '1'} checked {/if}>                                            <label for="manager_audited_1" class="ui-radio-label">{$lang.yes}</label>                                        </div>                                        <div class="checkbox_item">                                            <input type="radio" class="ui-radio" name="manager_audited" id="manager_audited_2" value="0" {if $partner_company_config.manager_audited === '0'} checked {/if}>                                            <label for="manager_audited_2" class="ui-radio-label">{$lang.no}</label>                                        </div>                                    </div>                                </div>                            </div>                            <div class="item">                                <div class="label">{$lang.partner_goods_not_payment}：</div>                                <div class="label_value">                                    <div class="checkbox_items">                                        <div class="checkbox_item">                                            <input type="radio" class="ui-radio"  name="not_payment" id="not_payment_1" value="1" {if $partner_company_config.not_payment === '1'} checked {/if}>                                            <label for="not_payment_1" class="ui-radio-label">{$lang.yes}</label>                                        </div>                                        <div class="checkbox_item">                                            <input type="radio" class="ui-radio" name="not_payment" id="not_payment_2" value="0" {if $partner_company_config.not_payment === '0'} checked {/if}>                                            <label for="not_payment_2" class="ui-radio-label">{$lang.no}</label>                                        </div>                                    </div>                                </div>                            </div>                            <div class="item">                                <div class="label">&nbsp;</div>                                <div class="label_value info_btn">                                    <input type="submit" value="{$lang.button_submit}" class="button" />                                    <input type="reset" value="{$lang.button_reset}" class="button button_reset" />                                </div>                            </div>                        </div>                    </form>                </div>            </div>        </div>    </div></div>{include file ='library/pagefooter.lbi'}</body></html>