{if $full_page}<!doctype html><html><head>{include file ='library/admin_html_head.lbi'}</head><body class="iframe_body"><div class="warpper">    <div class="title">{$lang.goods_alt} - {$ur_here}</div>    <div class="content">        <div class="tabs_info">            <ul>                <li class="{if $act == 'partner_allocated_goods'}curr{/if}">                    <a href="partner.php?act=partner_allocated_goods&id={$user_id}&user={$user_name}">{$lang.allocated_goods}</a>                </li>                <li class="{if $act == 'partner_platform_goods'}curr{/if}">                    <a href="partner.php?act=partner_platform_goods&id={$user_id}&user={$user_name}">{$lang.platform_goods}</a>                </li>            </ul>        </div>        <div class="explanation mb10" id="explanation">            <div class="ex_tit"><i class="sc_icon"></i><h4>{$lang.operating_hints}</h4><span id="explanationZoom" title="{$lang.fold_tips}"></span></div>            <ul>                <li>{$lang.partner_operation_prompt_content.list.0}</li>                <li>{$lang.partner_operation_prompt_content.list.1}</li>                <li>{$lang.partner_operation_prompt_content.list.4}</li>            </ul>        </div>        <div class="flexilist">            <!--商品列表-->            <div class="common-head">                <div class="refresh">                    <div class="refresh_tit" title="{$lang.refresh_data}"><i class="icon icon-refresh"></i></div>                    <div class="refresh_span">{$lang.refresh_common}{$record_count}{$lang.record}</div>                </div>                <div class="search">                    <form action="javascript:;" name="searchForm" onSubmit="searchGoodsname(this);" class="fr">                        <div class="input">                            <input type="text" name="keyword" class="text nofocus w140" placeholder="{$lang.search_keyword_placeholder}" autocomplete="off">                            <input type="submit" class="btn" name="secrch_btn" ectype="secrch_btn" value="" />                        </div>                    </form>                </div>            </div>            <div class="common-content">                <form method="post" action="" name="listForm" onsubmit="return confirmSubmit(this)">                    <div class="list-div" id="listDiv">                        {/if}                        <div class="flexigrid ht_goods_list{if $add_handler} xn_goods_list{/if}">                            <table cellpadding="0" cellspacing="0" border="0">                                <thead>                                <tr>                                    <th width="3%" class="sign"><div class="tDiv"><input type="checkbox" name="all_list" class="checkbox" id="all_list" /><label for="all_list" class="checkbox_stars"></label></div></th>                                    <th width="5%" class="sky_id"><div class="tDiv"><a href="javascript:listTable.sort('goods_id');">{$lang.record_id}</a>{$sort_goods_id}</div></th>                                    <th width="18%"><div class="tDiv"><a href="javascript:listTable.sort('goods_name');">{$lang.goods_name}</a>{$sort_goods_name}</div></th>                                    <th width="8%"><div class="tDiv">{$lang.goods_steps_name}</div></th>                                    <th width="3%"><div class="tDiv">{$lang.goods_price}</div></th>                                    <th width="3%"><div class="tDiv">{$lang.exclusive_goods_price}</div></th>                                    <th width="3%"><div class="tDiv">{$lang.goods_sn}</div></th>                                    <th width="10%" class="handle">{$lang.handler}</th>                                </tr>                                </thead>                                <tbody>                                {foreach from=$goods_list item=goods}                                <tr>                                    <td class="sign">                                        <div class="tDiv">                                            <input type="checkbox" name="checkboxes[]" value="{$goods.goods_id}" data-goods_name="{$goods.goods_name}" class="checkbox" id="checkbox_{$goods.goods_id}" />                                            <label for="checkbox_{$goods.goods_id}" class="checkbox_stars"></label>                                        </div>                                    </td>                                    <td class="sky_id"><div class="tDiv">{$goods.goods_id}</div></td>                                    <td>                                        <div class="tDiv goods_list_info">                                            <div class="img"><a href="../goods.php?id={$goods.goods_id}" target="_blank" title="{$goods.goods_name|escape:html}"><img src="{$goods.goods_thumb}" width="68" height="68" /></a></div>                                            <div class="desc">                                                <div class="name">                                                    <span title="{$goods.goods_name|escape:html}" data-toggle="tooltip" class="span">{$goods.goods_name|escape:html}</span>                                                </div>                                                {if $goods.brand_name}<p class="brand">{$lang.brand}：<em>{$goods.brand_name}</em></p>{/if}                                                <p class="activity">                                                    {if $goods.is_shipping}                                                    <em class="free">{$alng.free_shipping_alt}</em>                                                    {/if}                                                    {if $goods.stages}                                                    <em class="byStage">{$lang.by_stages}</em>                                                    {/if}                                                    {if !$goods.is_alone_sale}                                                    <em class="parts">{$lang.tab_groupgoods}</em>                                                    {/if}                                                    {if $goods.is_promote}                                                    {if $nowTime gte $goods.promote_end_date}                                                    <em class="saleEnd">{$lang.promote_end_date}</em>                                                    {else}                                                    <em class="sale">{$lang.promote_date}</em>                                                    {/if}                                                    {/if}                                                    {if $goods.is_xiangou}                                                    {if $nowTime gte $goods.xiangou_end_date}                                                    <em class="purchaseEnd">{$lang.xiangou_end}</em>                                                    {else}                                                    <em class="purchase">{$lang.xiangou}</em>                                                    {/if}                                                    {/if}                                                    {if $goods.is_distribution}                                                    <em class="distribution">{$lang.distribution}</em>                                                    {/if}                                                </p>                                            </div>                                        </div>                                    </td>                                    <td>                                        <div class="tDiv">                                            <div class="goods_list_seller" title="{$goods.user_name}" data-toggle="tooltip">                                                <font class="red">{$goods.user_name}</font>                                            </div>                                        </div>                                    </td>                                    <td>                                        <div class="tDiv">                                            <div class="tDiv_item">                                                <span class="label">{$lang.shop_price}：</span>                                                <div class="value">                                                    <span>{$goods.shop_price}</span>                                                </div>                                            </div>                                        </div>                                    </td>                                    <td>                                        <div class="tDiv">                                            <div class="tDiv_item">                                                <span class="label">{$lang.shop_price}：</span>                                                <div class="value">                                                    <span class="red" id="exclusive_price_{$goods.goods_id}" style="font-weight:bold;">{$goods.exclusive_price}</span>                                                </div>                                            </div>                                        </div>                                    </td>                                    <td>                                        <div class="tDiv">                                            <div class="tDiv_item">                                                <span class="label">{$lang.goods_sn}：</span>                                                <div class="value">                                                    <span>{$goods.goods_sn}</span>                                                </div>                                            </div>                                        </div>                                    </td>                                    <td class="handle">                                        <div class="tDiv ht_tdiv" style="padding-bottom:0px;">                                            <p>                                                <a href="../goods.php?id={$goods.goods_id}" target="_blank" class="btn_see"><i class="sc_icon sc_icon_see"></i>{$lang.view}</a>                                                <a href="javascript:C.setExclusivePrice('{$goods.goods_name}','{$goods.goods_id}');" class="btn_edit" title='{$lang.copy_goods}'><i class="icon icon-share"></i>{$lang.set_exclusive_price}</a>                                            </p>                                        </div>                                    </td>                                </tr>                                {/foreach}                                </tbody>                                <tfoot>                                <tr>                                    <td colspan="12">                                        <div class="tDiv">                                            <div class="tfoot_btninfo">                                                <div class="checkbox_item fl font12 mt5 mr5">                                                    <input type="checkbox" name="all_list" class="ui-checkbox" id="all_list"><label for="all_list" class="ui-label">{$lang.check_all}</label>                                                </div>                                                <input type="hidden" name="act" value="batch" />                                                <!-- 操作类型 start -->                                                <div class="imitate_select select_w120">                                                    <div class="cite">{$lang.set_exclusive_price}</div>                                                    <ul>                                                        <li><a href="javascript:changeAction();" data-value="sync_goods" class="ftx-01">{$lang.set_exclusive_price}</a></li>                                                    </ul>                                                    <input name="type" type="hidden" value="" id="">                                                </div>                                                <!-- 操作类型 end -->                                                <input type="button" onclick="C.batchSetExclusivePrice()" value="{$lang.button_submit}" id="btnSubmit" name="btnSubmit" class="btn btn_disabled" disabled="true" ectype="btnSubmit" />                                            </div>                                            <div class="list-page">                                                {include file="library/page.lbi"}                                            </div>                                        </div>                                    </td>                                </tr>                                </tfoot>                            </table>                        </div>                        {if $full_page}                    </div>                </form>            </div>            <!--商品列表end-->        </div>    </div></div><!--高级搜索 start-->{include file="library/partner_platform_goods_search.lbi"}<!--高级搜索 end-->{include file ='library/pagefooter.lbi'}{insert_scripts files="jquery.purebox.js"}{literal}<script type="text/javascript">    listTable.query = "query_partner_allocated_goods";    listTable.recordCount = {$record_count|default:0};    listTable.pageCount = {$page_count|default:1};    {foreach from=$filter item=item key=key}    listTable.filter.{$key} = '{$item}';    {/foreach}        /* 起始页通过商品一览点击进入自营/商家商品判断条件 */        listTable.filter.self = '{$self}';        listTable.filter.merchants = '{$merchants}';        function movecatList(val, level) {            var cat_id = val;            document.getElementById('target_cat').value = cat_id;            Ajax.call('goods.php?is_ajax=1&act=sel_cat_goodslist', 'cat_id=' + cat_id + '&cat_level=' + level, movecatListResponse, 'GET', 'JSON');        }        function movecatListResponse(result) {            if (result.error == '1' && result.message != '') {                alert(result.message);                return;            }            var response = result.content;            var cat_level = result.cat_level;            for (var i = cat_level; i < 10; i++) {                $("#move_cat_list" + Number(i + 1)).remove();            }            if (response) {                $("#move_cat_list" + cat_level).after(response);            }            return;        }        onload = function () {            document.forms['listForm'].reset();        }        /**         * @param: bool ext 其他条件：用于转移分类         */        function confirmSubmit(frm, ext) {            if (frm.elements['type'].value == 'trash') {                return confirm(batch_trash_confirm);            } else if (frm.elements['type'].value == 'not_on_sale') {                return confirm(batch_no_on_sale);            } else if (frm.elements['type'].value == 'move_to') {                ext = (ext == undefined) ? true : ext;                return ext && document.getElementById('target_cat').value != 0;            } else if (frm.elements['type'].value == '') {                return false;            } else {                return true;            }        }        function changeAction() {            var frm = document.forms['listForm'];            // 切换分类列表的显示            $("#move_cat_list").css({'display': frm.elements['type'].value == 'move_to' ? '' : 'none'});            // 切换商品审核列表的显示            $("#review_status").css({'display': frm.elements['type'].value == 'review_to' ? '' : 'none'});            // 商品运费模板            $("#goods_transport").css({'display': frm.elements['type'].value == 'goods_transport' ? '' : 'none'});            if (frm.elements['type'].value != 'review_to') {                frm.elements['review_content'].style.display = 'none';            }            // 供应商列表的显示            {if $suppliers_list > 0}            $("#suppliers_id").css({'display': frm.elements['type'].value == 'suppliers_move_to' ? '' : 'none'});            {/if}            }            //ecmoban模板堂 --zhuo  start            function get_review_status() {                var frm = document.forms['listForm'];                if (frm.elements['type'].value == 'review_to') {                    if (frm.elements['review_status'].value == 2) {                        frm.elements['review_content'].style.display = '';                    } else {                        frm.elements['review_content'].style.display = 'none';                    }                } else {                    frm.elements['review_content'].style.display = 'none';                }            }            //ecmoban模板堂 --zhuo  end            //展开其他属性            function trigger(obj) {                var _this = $(obj);                var parenttr = _this.parents('tr');                var tip = parenttr.siblings().find('.tip');                if (_this.hasClass('icon-down')) {                    _this.removeClass('icon-down');                    parenttr.next().hide();                } else {                    _this.addClass('icon-down');                    parenttr.next().show();                    tip.removeClass('icon-down');                    tip.parents('tr').next().hide();                }            }            //单选勾选            function get_ajax_act(t, goods_id, act, FileName) {                if (t.checked == false) {                    t.value = 0;                }                Ajax.call(FileName + '.php?act=' + act, 'id=' + goods_id + '&val=' + t.value, act_response, 'POST', 'JSON');            }            function act_response(result) {            }            function dropWarehouse(w_id) {                Ajax.call('goods.php?is_ajax=1&act=drop_warehouse', "w_id=" + w_id, dropWarehouseResponse, "GET", "JSON");            }            function dropWarehouseResponse(result) {                if (result.error == 0) {                    document.getElementById('warehouse_' + result.content).style.display = 'none';                }            }</script><script>    var C = {        data: {goods_name: [], goods_ids: [], user_id: null, price: null},        layer_ele: null,        setExclusivePrice: function (goods_name, goods_id) {            this.data.goods_name =  [goods_name];            this.data.goods_ids = [goods_id];            this.data.user_id = "{$user_id}";            this.openDiv()        },        batchSetExclusivePrice: function () {            var _this = this;            _this.data.goods_ids = [];            _this.data.goods_name = [];            this.data.user_id = "{$user_id}";            $('.checkbox').each(function () {                var goods_id =  $(this).val();                var goods_name =  $(this).attr('data-goods_name');                if ($(this).prop('checked') && _this.isNumeric(goods_id)) {                    _this.data.goods_ids.push(goods_id);                    _this.data.goods_name.push(goods_name);                }            });            if( _this.data.goods_ids.length < 1) {                layer.msg('请选择');            }else{                this.openDiv();            }        },        openDiv: function () {            var _this = this;            var ele = '<div class="item" style="margin: 10px 10px 30px 10px;display: inline-block;">\n';            for (var i = 0; i < this.data.goods_name.length; i++) {                ele += '<div style="margin: 10px 0"><span class="require-field">*</span>' + this.data.goods_name[i] + '</div>';            }            ele += '   <input type="text" name="price" id="exclusive_price" class="text w140" placeholder="专属价格" autocomplete="off">\n' +                '    <a href="javascript:C.setPrice(\'#exclusive_price\');" class="btn red_btn h30 mr10 fl" style="line-height:30px;">设置</a>\n' +                '</div>';            _this.layer_ele = layer.open({                title: '设置',                type: 1,                area: ['auto', 'auto'],                shadeClose: false, //点击遮罩关闭                content: ele            });        },        setPrice: function (id_name) {            var _this = this;            var price = $(id_name).parent('.item').find('#exclusive_price').val();            if (_this.isNumeric(price) === false) {                layer.msg('输入的小于0！');            } else {                _this.data.price = price;                _this.callAjax();            }        },        callAjax: function () {            window._c_data =this.data;            Ajax.call('partner.php?act=set_exclusive_price', 'goods_ids=' + this.data.goods_ids + '&user_id=' + this.data.user_id + '&price=' + this.data.price, this.actResponse, 'POST', 'JSON');        },        actResponse: function (response) {            if (response.code == '200') {                layer.closeAll();                var goods_ids = window._c_data.goods_ids;                for (var i = 0; i < goods_ids.length; i++) {                    var exclusive_price_ele = '#exclusive_price_' + goods_ids[i];                    $(exclusive_price_ele).html(response.content.price);                    console.log(exclusive_price_ele)                }            }            layer.msg(response.message);        },        isNumeric: function (numeric) {            if (parseFloat(numeric) && numeric >= 0) {                return true;            } else {                return false;            }        }    };</script>{/literal}</body></html>{/if}